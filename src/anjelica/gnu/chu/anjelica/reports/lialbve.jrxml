and l.avc_ano = $P{avc_ano}
and l.avc_serie = $P{avc_serie}
and avl_canti >= 0
GROUP BY l.PRO_CODI,avl_prven,l.pro_nomb
UNION ALL
SELECT l.pro_codi,SUM(l.avl_canti) as avl_canti,
sum(avl_unid) as avl_unid,
