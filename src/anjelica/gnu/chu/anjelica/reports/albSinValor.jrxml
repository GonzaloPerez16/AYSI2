	<queryString><![CDATA[SELECT c.avc_fecalb,c.emp_codi,c.avc_ano,c.avc_serie,
c.avc_nume,c.cli_codi,cl.cli_nomb,l.pro_codi,a.pro_nomb,l.avl_canti
         FROM v_albavec as c,clientes as cl,v_albavel as l,
	v_articulo as a where
	cl.cli_codi = c.cli_codi
	and a.pro_codi = l.pro_codi
        and c.cli_codi = cl.cli_codi
        and c.emp_codi = l.emp_codi
        and c.avc_ano = l.avc_ano
        and c.avc_serie = l.avc_serie
        and c.avc_nume = l.avc_nume
        and l.avl_canti != 0
        and l.avl_prven = 0
       and l.pro_codi >= 100
and c.avc_fecalb >= '20031001'
        ORDER BY c.avc_fecalb,cli_codi]]></queryString>
